{
	"_version": "1.12.0",
	"sap.app": {
		"id": "ch.avectris.todo.TodoListClient",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"TodoBackend": {
				"uri": "/TodoBackend/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "ch.avectris.todo.TodoListClient.view.Master",
			"type": "XML",
			"async": true,
			"id": "Master"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ch.avectris.todo.TodoListClient.i18n.i18n"
				}
			},
			"todo_service": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "TodoBackend",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "ch.avectris.todo.TodoListClient.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "TargetMaster",
				"pattern": "RouteMaster",
				"target": ["TargetMaster"]
			}, {
				"name": "NewTodo",
				"pattern": "new",
				"titleTarget": "",
				"greedy": false,
				"target": ["NewTodo"]
			}, {
				"name": "Detail",
				"pattern": "detail/{todoId}",
				"titleTarget": "",
				"greedy": false,
				"target": ["Detail"]
			}],
			"targets": {
				"TargetMaster": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "master",
					"viewName": "Master",
					"viewLevel": 1
				},
				"NewTodo": {
					"viewType": "XML",
					"viewName": "NewTodo",
					"viewLevel": 2,
					"viewId": "newtodo"
				},
				"Detail": {
					"viewType": "XML",
					"viewName": "Detail",
					"viewId": "detail",
					"viewLevel": 2
				}
			}
		}
	}
}